{
  "name": "@leichtgewicht/dns-packet",
  "version": "5.4.1",
  "description": "An abstract-encoding compliant module for encoding / decoding DNS packets",
  "author": "Mathias Buus",
  "license": "MIT",
  "repository": "martinheidegger/dns-packet",
  "homepage": "https://github.com/martinheidegger/dns-packet",
  "main": "./index.cjs",
  "module": "./index.mjs",
  "types": "./index.d.ts",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "import": "./index.mjs",
      "require": "./index.cjs"
    },
    "./classes.js": {
      "types": "./classes.d.ts",
      "import": "./classes.mjs",
      "require": "./classes.cjs"
    },
    "./opcodes.js": {
      "types": "./opcodes.d.ts",
      "import": "./opcodes.mjs",
      "require": "./opcodes.cjs"
    },
    "./optioncodes.js": {
      "types": "./optioncodes.d.ts",
      "import": "./optioncodes.mjs",
      "require": "./optioncodes.cjs"
    },
    "./rcodes.js": {
      "types": "./rcodes.d.ts",
      "import": "./rcodes.mjs",
      "require": "./rcodes.cjs"
    },
    "./types.js": {
      "types": "./types.d.ts",
      "import": "./types.mjs",
      "require": "./types.cjs"
    }
  },
  "engines": {
    "node": ">=6"
  },
  "scripts": {
    "clean": "rm -rf coverage",
    "lint": "standard && (cd types; tsc)",
    "test": "npm run lint && npm run unit",
    "unit": "tape test.mjs",
    "coverage": "c8 -r html npm test",
    "prepare": "npx @leichtgewicht/esm2umd"
  },
  "dependencies": {
    "@leichtgewicht/ip-codec": "^2.0.4",
    "typescript": "^4.6.4"
  },
  "devDependencies": {
    "@leichtgewicht/esm2umd": "^0.1.1",
    "@types/node": "^17.0.34",
    "c8": "^7.11.2",
    "standard": "^17.0.0",
    "tape": "^5.5.3"
  },
  "keywords": [
    "dns",
    "packet",
    "encodings",
    "encoding",
    "encoder",
    "abstract-encoding"
  ]
}
